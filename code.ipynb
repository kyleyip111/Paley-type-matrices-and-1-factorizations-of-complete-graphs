N = 92000000    # 9.2e7

def distinct_prime_factors(n):
    fac = []
    d = 2
    while d*d <= n:
        if n % d == 0:
            fac.append(d)
            while n % d == 0:
                n //= d
        d = 3 if d == 2 else d + 2
    if n > 1:
        fac.append(n)
    return fac

def has_good_a(p):
    m = p - 1
    fac = distinct_prime_factors(m)          # prime factors of p-1
    for a in range(2, p-1):
        # primitive root test
        if any(pow(a, m//q, p) == 1 for q in fac):
            continue
        x = (a * (a*a - a + 1)) % p
        if x != 0 and pow(x, m//4, p) == 1:  # 4th power test (pâ‰¡1 mod4)
            return p, a
    return False, p, None

